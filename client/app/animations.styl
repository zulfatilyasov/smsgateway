@keyframes display {
  from {
    -webkit-transform: scale(1,0);
  }
  to {
    -webkit-transform: scale(1,1);
  }
}

@-o-keyframes display {
  from {
    -webkit-transform: scale(1,0);
  }
  to {
    -webkit-transform: scale(1,1);
  }
}

@-ms-keyframes display {
  from {
    -webkit-transform: scale(1,0);
  }
  to {
    -webkit-transform: scale(1,1);
  }
}

@-moz-keyframes display {
  from {
    -webkit-transform: scale(1,0);
  }
  to {
    -webkit-transform: scale(1,1);
  }
}

@-webkit-keyframes display {
  from {
    -webkit-transform: scale(1,0);
  }
  to {
    -webkit-transform: scale(1,1);
  }
}

.animated{
  -webkit-transform: scale(1,0);
  -ms-transform: scale(1,0);
  -o-transform: scale(1,0);
  transform: scale(1,0);
}

.animated.active {
  -webkit-animation: display 0.5s cubic-bezier(0.55, 0, 0.1, 1) forwards;
  -o-animation: display 0.5s cubic-bezier(0.55, 0, 0.1, 1) forwards;
  animation: display 0.5s cubic-bezier(0.55, 0, 0.1, 1) forwards;
}

.fadeDown2-enter
  opacity: 0
  transform: translateY(15px)translateZ(0)
  transition: transform .35s , opacity .35s
 
.fadeDown2-enter.fadeDown2-enter-active 
  transform: translateY(0px)translateZ(0)
  transition-delay: .09625s;
  opacity: 1

.fadeDown-enter
  opacity: 0
  transform: translateY(15px)translateZ(0)
  transition: transform .35s , opacity .35s

.fadeDown-enter.fadeDown-enter-active 
  transform: translateY(0px)translateZ(0)
  opacity: 1